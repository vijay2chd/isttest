pipeline {
  agent any

  stages {
    stage('Compile') {
       steps {
                sh "mvn compile"
       }
    }

    stage('Build') {
             steps {
          sh "mvn package"
             }
    }
       
     stage('Install') {
             steps {
          sh "mvn install"
             }
    }

     stage('Archieve') {
             steps {
          archiveArtifacts 'target/*.jar'
             }
    }
     stage('FingerPrint') {
             steps {
             fingerprint 'target/*.jar'
             }
    }

 }

}

